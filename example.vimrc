" This is a comment!

" Sets the line numbers on the left
set nu

" Turns on syntax hilighting
syntax on

" Save
nmap <leader>w :w<CR>
" Save and exit
nmap <leader>x :x<CR>
" Exit
nmap <leader>q :q<CR>

nmap <leader>tn :tabnew<CR>
nmap ;h :tabp<CR>
nmap ;l :tabn<CR>

map <C-j> <C-W>j
map <C-k> <C-W>k
map <C-l> <C-W>l
map <C-h> <C-W>h
